{
  "name": "galaxyco-ai-v2-api",
  "buildCommand": "npm run build --workspace=api",
  "outputDirectory": "apps/api/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "apps/api/dist/main.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/api/dist/main.js"
    }
  ]
}